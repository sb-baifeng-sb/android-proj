<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ui" >

    <application
        android:name="com.example.application.BaseApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity

            android:name="com.example.ui.UiSlashActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.ui.UiStartActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.ui.UiMyFramentActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.ui.UiLoginActivity"
            android:label="@string/login"
            android:parentActivityName="com.example.ui.UiStartActivity">
        </activity>
        <activity
            android:name="com.example.ui.UiRegisterActivity"
            android:parentActivityName="com.example.ui.UiStartActivity"
            android:label="@string/register"
            >
        </activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"></uses-permission>
</manifest>
